{
    "go.formatTool": "goimports",
    "vim.leader": "<space>",
    "vim.insertModeKeyBindings": [
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["J"],
            "after": ["5", "j"]
        },
        {
            "before": ["K"],
            "after": ["5", "k"]
        },
        {
            "before": [";"],
            "after": [":"]
        },{
            "before": ["<leader>", "h"],
            "after": ["<C-w>", "h"]
        },{
            "before": ["<leader>", "l"],
            "after": ["<C-w>", "l"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["t", "h"],
            "commands": [":tabprevious"]
        },
        {
            "before": ["t", "l"],
            "commands": [":tabnext"]
        },
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": ["J"],
            "after": ["5", "j"]
        },
        {
            "before": ["K"],
            "after": ["5", "k"]
        }
    ]
}